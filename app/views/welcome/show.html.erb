WELCOME! This isn't a blog.
<p id="notice"><%= notice %></p>

<h1>Notposts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>User</th>
      <th>Content</th>
      <th>Image</th>
      <th>Vote Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td>Title:<%= post.title %></td>
        <td>User:<%= post.user_id %></td>
        <td>Content:<%= post.content %></td>
        <td>Image:<%= post.image_url %></td>


        <td>
        <%= form_for(@vote) do |f| %>
        <%= f.button "Upvote" %>
        <%= f.hidden_field :vote_type, :value => "upvote" %>
        <%= f.hidden_field :post_id, :value => post.id %>
        <% end %></td>
        <td>
        <%= form_for(@vote) do |f| %>
        <%= f.button "Downvote" %>
        <%= f.hidden_field :vote_type, :value => "downvote" %>
        <%= f.hidden_field :post_id, :value => post.id %>
        <% end %></td>

        <%= post.title %> <%= post.get_upvotes.size %>

        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
